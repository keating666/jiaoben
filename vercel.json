{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "functions": {
    "api/video/transcribe.ts": {
      "maxDuration": 60,
      "includeFiles": "tech-validation/**"
    },
    "api/**/*.ts": {
      "includeFiles": "tech-validation/**"
    }
  }
}