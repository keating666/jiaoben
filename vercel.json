{
  "regions": ["hkg1"],
  "buildCommand": "npm run build || echo 'No build script'",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    },
    "api/**/*.ts": {
      "maxDuration": 10,
      "includeFiles": "tech-validation/**"
    },
    "api/video/transcribe.ts": {
      "maxDuration": 60,
      "includeFiles": "tech-validation/**"
    },
    "api/douyin/complete-process.js": {
      "maxDuration": 60
    },
    "api/test-yunmao-api.js": {
      "maxDuration": 60
    },
    "api/video/submit-optimized.js": {
      "maxDuration": 60
    },
    "api/test-region.js": {
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/hk-test.html",
      "dest": "/hk-test.html"
    },
    {
      "src": "/test-hk-deployment.html",
      "dest": "/test-hk-deployment.html"
    },
    {
      "src": "/test-api.html",
      "dest": "/test-api.html"
    },
    {
      "src": "/test-yunmao-two-step.html",
      "dest": "/test-yunmao-two-step.html"
    },
    {
      "src": "/test-douyin-hk.html",
      "dest": "/public/test-douyin-hk.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}