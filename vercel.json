{
  "functions": {
    "api/video/transcribe.ts": {
      "maxDuration": 60,
      "includeFiles": "tech-validation/**"
    },
    "api/test-components.ts": {
      "maxDuration": 30,
      "includeFiles": "tech-validation/**"
    },
    "api/**/*.ts": {
      "maxDuration": 10,
      "includeFiles": "tech-validation/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}