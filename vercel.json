{
  "git": {
    "deploymentEnabled": false
  },
  "functions": {
    "api/video/transcribe.ts": {
      "maxDuration": 60,
      "includeFiles": "tech-validation/**"
    },
    "api/**/*.ts": {
      "includeFiles": "tech-validation/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}