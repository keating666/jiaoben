{
  "version": 2,
  "name": "jiaoben-tech-validation",
  "builds": [
    {
      "src": "tech-validation/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/tech-validation/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/tech-validation/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  },
  "functions": {
    "tech-validation/dist/api/*.js": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "regions": ["sin1"],
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}